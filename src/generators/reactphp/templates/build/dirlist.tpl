    <h1>Directory List</h1>
    <table class="table table-bordered table-striped" width="98%">
      <tr>
        <th>Name</th>
        <th>Size</th>
        <th>Last Modified</th>
      </tr>
      <tr>
        <td><a href="..">..</a></td>
        <td>4096</td>
        <td>&nbsp;</td>
      </tr>
      {{#files}}
      <tr>
        <td><a href="{{url}}">{{name}}</a></td>
        <td>{{size}}</td>
        <td>{{mtime}}</td>
      </tr>
      {{/files}}
    </table>
