
@require './font/iconfont.css'
@require './config'

initialize()

html,
body
    min-height: 100%

body
    -webkit-touch-callout: none
    -webkit-user-select: none
    background: $test-color

.aaa
    color: red
    background: url('./img/react-logo.png')
    background-size: contain
    background-repeat: no-repeat
    size(50px, 50px)

.ccc
    display: block
    background: url('./img/react-logo.png')
    background-size: contain
    background-repeat: no-repeat
    size(50px, 50px)

a,
a:link,
a:hover,
a:active,
a:focus
    color: inherit
    text-decoration: none

.cut-off
    ellipsis()
